<!DOCTYPE html>
<html lang="pt-BR">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Foodfy</title>
  <link href="https://fonts.googleapis.com/icon?family=Material+Icons"
      rel="stylesheet">
  <link rel="stylesheet" href="css/styles.css">
</head>
<body>

  <div class="container">

    <%- include('parts/page-header.ejs') %> 

  <div class="cards">

    <% recipes.forEach(function(recipe) { %>

      <div class="card" id="burger.png">
        <div class="card-image">
          <img src="<%= recipe.image %> " alt="image <%= recipe.title %>">
        </div>
        <div class="card-content">
          <p class="title_recipe"><%= recipe.title %></p>
          <p class="card_chef">por <%= recipe.author %></p>
        </div>
      </div>

      <% }); %> 

  </div>

  </div>


  <%- include('parts/page-footer.ejs') %> 

  <script src="script/scripts.js"></script>

</body>
</html>